{
  "name": "STVBallot",
  "description": "STV Ballot",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "version": "0.0.1",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "sandbox": {
     "pages": ["index.html"]
  },
  "permissions": [
    "experimental",
    {"socket": [
        "udp-send-to"
    ]},
    "app.window"
  ],
  "icons": { "16": "favicon.png", "128": "icon.png"}
}
