{
  "name": "STVBallot",
  "description": "STV Ballot",
  "manifest_version": 2,
  "version": "0.0.1",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "sandbox": {
     "pages": ["index.html"]
  },
  "permissions": ["experimental", "experimental.socket", "app.window"],
  "icons": { "16": "favicon.png", "32": "favicon.png", "128": "favicon.png"}
}
